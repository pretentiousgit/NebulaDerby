{
	"Socket":{
		 "conditions":[
				{
					 "id":"on-data-received",
					 "c2id":"0",
					 "scriptName":"OnData",
					 "isTrigger":true,
					 "highlight":true
				},
				{
					 "id":"on-connect",
					 "c2id":"1",
					 "scriptName":"OnConnect",
					 "isTrigger":true
				},
				{
					 "id":"on-error",
					 "c2id":"2",
					 "scriptName":"OnError",
					 "isTrigger":true
				},
				{
					 "id":"on-disconnect",
					 "c2id":"3",
					 "scriptName":"OnDisconnect",
					 "isTrigger":true
				},
				{
					 "id":"is-data-available",
					 "c2id":"4",
					 "scriptName":"IsDataAvailable",
					 "isTrigger":false
				},
				{
					 "id":"on-event-received",
					 "c2id":"5",
					 "scriptName":"OnEvent",
					 "isFakeTrigger":true,
					 "params":[
						 {"id":"socket-event", "type": "string"}
					]
				},
				{
					 "id":"on-any-event",
					 "c2id":"6",
					 "scriptName":"OnAnyEvent",
					 "isTrigger":true
				}
		 ],
		 "actions":[
				{
					 "id":"connect",
					 "c2id":"0",
					 "scriptName":"Connect",
					 "params":[
							{
									"id":"address",
									"type":"string",
									"description":"The address (eg. URL or IP) to connect to. Supports cross-domain requests.",
									"initial_str":"http:\/\/localhost"
							},
							{
								 "id":"port",
								 "type":"number",
								 "description":"The port to try and connect to the address through. This should be specific to your server.",
								 "initial_str":8000
							}
					 ]
				},
				{
					 "id":"disconnect",
					 "c2id":"1",
					 "scriptName":"Disconnect"
				},
				{
					 "id":"send",
					 "c2id":"2",
					 "scriptName":"Send",
					 "params":[
						{
								"id":"send-data",
								"type":"any"
						}
				 ]
				},
				{
					 "id":"splitDataReceived",
					 "c2id":"3",
					 "scriptName":"SplitDataReceived"
				},
				{
					 "id":"emit",
					 "c2id":"4",
					 "scriptName":"Emit",
					 "params": [
						 { "id":"emit-event" , "type": "any" },
						 { "id":"emit-data" , "type": "any" }
					 ]
				}
		 ],
		 "expressions":[
				{
					 "c2id":"0",
					 "id":"get-last-address",
					 "expressionName":"LastAddress",
					 "returnType":"string"
				},
				{
					 "c2id":"1",
					 "id":"get-last-port",
					 "expressionName":"LastPort",
					 "returnType":"string"
				},
				{
					 "c2id":"2",
					 "id":"get-last-data",
					 "expressionName":"LastData",
					 "returnType":"string"
				},
				{
					 "c2id":"3",
					 "id":"get-recieved-data-element",
					 "expressionName":"LastDataElement",
					 "returnType":"string"
				},
				{
					 "c2id":"4",
					 "id":"get-last-socket-event",
					 "expressionName":"LastSocketEvent",
					 "returnType":"string"
				},
				{
					 "c2id":"5",
					 "id":"get-last-emitted-socket-event",
					 "expressionName":"LastSocketEmittedEvent",
					 "returnType":"string",
					 "params": [
						 { "id": "last-emitted-event-index", "type": "number" }
					 ]
				}
		 ]
	}
}